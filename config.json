{
    "description": "Flannel plugin for Docker vDEV",
    "documentation": "https://github.com/sovarto/docker-network-plugin-flannel/",
    "entrypoint": [
        "/usr/bin/tini",
        "--",
        "/network-plugin-flannel"
    ],
    "env": [
        {
            "name": "ETCD_ENDPOINTS",
            "settable": [
                "value"
            ],
            "value": ""
        },
        {
            "name": "ETCD_PREFIX",
            "settable": [
                "value"
            ],
            "value": "/flannel"
        },
        {
            "name": "DEFAULT_FLANNEL_OPTIONS",
            "settable": [
                "value"
            ],
            "value": ""
        },
        {
            "name": "AVAILABLE_SUBNETS",
            "settable": [
                "value"
            ],
            "value": ""
        },
        {
            "name": "NETWORK_SUBNET_SIZE",
            "settable": [
                "value"
            ],
            "value": "20"
        },
        {
            "name": "DEFAULT_HOST_SUBNET_SIZE",
            "settable": [
                "value"
            ],
            "value": "25"
        }
    ],
    "network": {
        "type": "host"
    },
    "propagatedMount": "/flannel-env",
    "interface": {
        "types": [
            "docker.networkdriver/1.0"
        ],
        "socket": "flannel-np.sock"
    },
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN",
            "CAP_NET_ADMIN",
            "CAP_SYS_MODULE"
        ]
    }
}
